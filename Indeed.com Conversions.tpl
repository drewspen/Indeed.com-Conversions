// Indeed Conversion Tracking Tag Template
// This template allows you to track conversions for Indeed job postings.

// Access the API for the template directly
const log = require('log');
const injectScript = require('injectScript');
const callInWindow = require('callInWindow');
//const templateTag = require('templateTag'); // Not instantiated with `new` here

// Template parameters (defined in the Fields tab of the template editor)
const conversionId = data.conversionId;
const conversionLabel = data.conversionLabel || ''; // Label is optional
const conversionUrl = data.conversionUrl; 

// Validate required parameter
if (!conversionId) {
  log.error('Indeed Conversion ID is required.');
  data.gtmOnFailure();
  return;
}

// Inject the Indeed conversion script
injectScript( conversionUrl , () => {
  // Script loaded successfully, now execute the conversion tracking
  callInWindow((id, label) => {
    /* <![CDATA[ */
    var indeed_conversion_id = id;
    var indeed_conversion_label = label;
    /* ]]> */
  }, conversionId, conversionLabel);

  data.gtmOnSuccess(); // Notify GTM that the tag fired successfully
}, () => {
  // Handle script loading failure
  log.error('Failed to load Indeed conversion script.');
  data.gtmOnFailure(); // Notify GTM of failure
});


